{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "targets": {
          "node": "current"
        },
        "modules": false,
        "useBuiltIns": "usage",
        "exclude": [
          "transform-regenerator"
        ]
      }
    ],
    [
      "@babel/typescript"
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}